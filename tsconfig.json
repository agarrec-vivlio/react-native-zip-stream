{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "jsx": "react",
      "typeRoots": ["./node_modules/@types", "./@types"]
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }